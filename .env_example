# DataLabTo API Key
# Get your API key from https://www.datalab.to/app/dashboard
# Required for document conversion workflow
DATALABTO_API_KEY=your_datalab_api_key_here

# Google Gemini API Key
# Get your API key from https://aistudio.google.com/app/apikey
# Optional: Currently not used in the workflow, but may be needed for future features
GEMINI_API_KEY=your_gemini_api_key_here

# n8n API Configuration (for MCP workflow management)
# Get your API key from n8n: Settings → API → Create API Key
# Required for updating workflows via API (update_workflow_to_n8n.py)
# Default URL for local n8n instance
N8N_API_URL=http://localhost:5678
N8N_API_KEY=your_n8n_api_key_here
